pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                echo "Application Cloned Successfully"
            }
        }
        stage('Install') {
            steps {
                echo "Dependencies Installed Successfully"
            }
        }
        stage('Build') {
            steps {
                echo "Application Build Successfully"
            }
        }
        stage('Test') {
            steps {
                echo "Test Cases Executed Successfully"
            }
        }
        stage('Deploy') {
            steps {
                echo "Application is successfully deployed"
            }
        }
    }
}
